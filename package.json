{
  "name": "swf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "// --- develop --- //": "",
    "start": "react-native start",
    "bootstrap": "yarn && cd ios && npx pod install && cd ../",
    "dev": "yarn adb && react-native start",
    "adb": "(adb reverse tcp:8081 tcp:8081 || true) && (adb reverse tcp:7007 tcp:7007 || true)",
    "android": "adb shell pm uninstall --user 0 com.swf ; react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15 Pro'",
    "// --- test --- //": "",
    "test": "jest --passWithNoTests",
    "// --- tools --- //": "",
    "pull": "git pull origin develop",
    "merge": "git merge origin develop",
    "pods": "cd ios && pod install && cd ..",
    "pods:m1": "cd ios && arch -x86_64 pod install && cd ..",
    "android:clear": "cd android && ./gradlew clean && cd ..",
    "ios:clear": "cd ios && rm -rf ./Podfile.lock && rm -rf ./Pods && cd ..",
    "// --- linters and formatters --- //": "",
    "lint": "eslint src/** --fix --no-error-on-unmatched-pattern",
    "format": "prettier --write '**/*.(ts|tsx)'",
    "validate": "tsc && yarn lint && pretty-quick --staged"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@shopify/flash-list": "^1.6.3",
    "@tanstack/react-query": "^5.8.1",
    "@testing-library/react-native": "^12.3.1",
    "@types/ms": "^0.7.33",
    "@types/qs": "^6.9.9",
    "axios": "^0.27.2",
    "axios-logger": "^2.6.1",
    "i18next": "^20.6.1",
    "lottie-react-native": "^6.4.0",
    "mobx": "^6.11.0",
    "mobx-react-lite": "^4.0.5",
    "ms": "^2.1.3",
    "qs": "^6.11.2",
    "react": "18.2.0",
    "react-i18next": "^11.11.2",
    "react-native": "^0.72.6",
    "react-native-config": "^1.5.1",
    "react-native-flipper": "^0.212.0",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-localize": "^2.1.1",
    "react-native-reanimated": "^3.4.2",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-svg": "^13.14.0",
    "react-native-svg-transformer": "^1.1.0",
    "tailwindcss": "^3.3.5",
    "twrnc": "^3.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.6",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "react-native-flipper-performance-plugin": "^0.4.0",
    "react-query-native-devtools": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
